{
  "name": "Calaos",
  "id": "calaos",
  "description": {
    "en": "Calaos Home Automation"
  },
  "developer": {
    "name": "naguirre",
    "email": "nico@calaos.fr",
    "url": "http://calaos.fr"
  },
  "multi_instance": "false",
  "arguments": {
    "install": [
      {
        "name": "domain",
        "ask": {
          "en": "Choose a domain for Calaos"
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "ask": {
          "en": "Choose a path for Calaos"
        },
        "example": "/calaos",
        "default": "/calaos"
      },
      {
        "name": "server",
        "ask": {
          "en": "Choose IP where Calaos is installed"
        },
        "example": "127.0.0.1",
        "default": "127.0.0.1"
      }
    ]
  }
}
